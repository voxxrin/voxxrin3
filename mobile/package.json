{
  "name": "voxxrin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel vite-dev typesafe-i18n",
    "vite-dev": "vite --mode dev",
    "build-dev": "vite --mode dev build",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "run-dev-android": "NODE_ENV=dev npx cap sync android && NODE_ENV=dev npx cap run android",
    "run-dev-ios": "NODE_ENV=dev npx cap sync ios && NODE_ENV=dev npx cap run ios",
    "buildrun-dev-android": "npm run build-dev && npm run run-dev-android",
    "buildrun-dev-ios": "npm run build-dev && npm run run-dev-ios",
    "run-prod-android": "NODE_ENV=prod npx cap sync android && NODE_ENV=prod npx cap run android",
    "run-prod-ios": "NODE_ENV=prod npx cap sync ios && NODE_ENV=prod npx cap run ios",
    "buildrun-prod-android": "npm run build && npm run run-prod-android",
    "buildrun-prod-ios": "npm run build && npm run run-prod-ios",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@capacitor/android": "5.0.0",
    "@capacitor/app": "5.0.0",
    "@capacitor/browser": "5.0.0",
    "@capacitor/core": "5.0.0",
    "@capacitor/haptics": "5.0.0",
    "@capacitor/ios": "5.0.0",
    "@capacitor/keyboard": "5.0.0",
    "@capacitor/splash-screen": "5.0.0",
    "@capacitor/status-bar": "5.0.0",
    "@ionic/vue": "7.0.4",
    "@ionic/vue-router": "7.0.4",
    "@vueuse/core": "^10.1.2",
    "firebase": "^9.22.0",
    "ionicons": "7.0.0",
    "temporal-polyfill": "0.1.1",
    "ts-pattern": "4.2.2",
    "typesafe-i18n": "5.24.3",
    "vue": "3.2.47",
    "vue-router": "4.1.6",
    "vuefire": "3.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "5.0.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.3.2",
    "cypress": "12.7.0",
    "eslint": "8.35.0",
    "eslint-plugin-vue": "9.9.0",
    "jsdom": "21.1.1",
    "npm-run-all": "4.1.5",
    "sass": "1.62.1",
    "terser": "5.17.3",
    "ts-essentials": "9.3.1",
    "typescript": "5.0.4",
    "vite": "4.1.4",
    "vite-plugin-pwa": "0.14.7",
    "vitest": "0.30.1",
    "vue-tsc": "1.0.24",
    "workbox-build": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "description": "An Ionic project",
  "license": "Apache-2.0"
}
