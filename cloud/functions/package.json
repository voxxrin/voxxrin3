{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && env-cmd -f ./.env.local firebase emulators:start --import data --export-on-exit --inspect-functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase use ${FIREBASE_PROJECT_NAME:-voxxrin-v3} && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "vitest"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/cloud/functions/src/index.js",
  "dependencies": {
    "@googleapis/youtube": "15.0.0",
    "@js-temporal/polyfill": "^0.4.3",
    "@types/lodash": "^4.14.194",
    "axios": "^1.3.6",
    "cheerio": "1.0.0-rc.12",
    "express": "4.19.2",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "4.8.2",
    "lodash": "^4.17.21",
    "string-similarity-js": "2.1.4",
    "ts-pattern": "5.1.1",
    "uuid": "9.0.0",
    "yaml": "2.3.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "13.6.0",
    "ts-essentials": "9.4.1",
    "typescript": "5.4.5",
    "vitest": "0.31.1"
  },
  "private": true
}
