{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && MIGRATION_TOKEN=42 firebase emulators:start  --import data --export-on-exit --inspect-functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase use voxxrin-v3 && firebase deploy --only functions",
    "deploy:demo": "firebase use voxxrin-v3-demo && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "vitest"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.3",
    "@types/lodash": "^4.14.194",
    "axios": "^1.3.6",
    "cheerio": "1.0.0-rc.12",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.0",
    "lodash": "^4.17.21",
    "ts-pattern": "4.3.0",
    "uuid": "9.0.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "12.0.0",
    "typescript": "^4.9.0",
    "vitest": "0.31.1"
  },
  "private": true
}
